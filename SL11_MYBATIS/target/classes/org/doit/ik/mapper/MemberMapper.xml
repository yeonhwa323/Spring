<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.doit.ik.mapper.MemberMapper">

	<!-- MemberVO getMember(String id) throws ClassNotFoundException, SQLException; -->
	<select id="getMember" resultType="org.doit.ik.domain.MemberVO">
		SELECT *
		FROM MEMBER 
		WHERE ID=#{id}
	</select>
	<!-- int insert(MemberVO member) throws ClassNotFoundException, SQLException; -->
	<insert id="insert">
		INSERT INTO MEMBER 
		(id, pwd, name, gender, birth, is_lunar, cphone , email, habit, regdate) 
		VALUES( #{id}, #{pwd}, #{name}, #{gender}, #{birth}, #{is_lunar}, #{cphone}, #{email}, #{habit}, SYSDATE)
	</insert>
  
</mapper>